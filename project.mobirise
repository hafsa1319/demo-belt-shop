          "_name": "shop04",
          "_customHTML": "<section class=\"amp-shop shop4\" group=\"Shop\" plugins=\"amp-main, amp-lightbox, amp-bind\">\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Main Title\" name=\"titleSection\">\n        <input type=\"checkbox\" name=\"showText\" title=\"Shop Text\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showcardTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Buttons\" name=\"showButtons\" checked=\"\">\n        <header condition=\"customStyle\">Style</header>\n        <input type=\"image\" title=\"Right Image\" name=\"bg2\" value=\"@PROJECT_PATH@/assets/images/mbr-1920x1275.jpg\">\n        <header condition=\"customStyle\">Shop Styles</header>\n        <input type=\"checkbox\" name=\"customStyles\" title=\"Custom Style\" checked=\"\">\n        <header condition=\"customStyles\">Card Color</header>\n        <input type=\"checkbox\" name=\"useColorCard\" title=\"Use Background Card\" condition=\"customStyles\" checked=\"\">\n        <input type=\"color\" name=\"colorCard\" title=\"Color\" value=\"#f5f5f5\" condition=\"useColorCard &amp;&amp; customStyles\">\n        <input type=\"range\" title=\"Opacity\" name=\"opacityCard\" min=\"0\" max=\"1\" step=\"0.1\" value=\"1\" condition=\"useColorCard &amp;&amp; customStyles\">\n        <input type=\"color\" name=\"fontColor\" title=\"Font Color\" value=\"#4a4a4a\" condition=\"customStyles\">\n        <input type=\"color\" name=\"btnColor\" title=\"Btn Color\" value=\"#232323\" condition=\"customStyles\">\n        <input type=\"color\" name=\"btnText\" title=\"Btn Text Color\" value=\"#ffffff\" condition=\"customStyles\">\n\n        <header condition=\"customStyles\">Paddings</header>\n        <input type=\"checkbox\" name=\"addPadding\" title=\"Add Padding\" condition=\"customStyles\" checked=\"\">\n        <input type=\"checkbox\" name=\"imgPadding\" title=\"No Image Padding\" condition=\"customStyles &amp;&amp; addPadding\">\n        <input type=\"range\" title=\"X Padding\" name=\"xPadding\" min=\"0\" max=\"5\" step=\"0.5\" value=\"2\" condition=\"addPadding &amp;&amp; customStyles\">\n        <input type=\"range\" title=\"Y Padding\" name=\"yPadding\" min=\"0\" max=\"5\" step=\"0.5\" value=\"2\" condition=\"addPadding &amp;&amp; customStyles\">\n\n        <header condition=\"customStyles\">Box Shadow</header>\n        <input type=\"checkbox\" name=\"boxShadow\" title=\"Shadow\" condition=\"customStyles\">\n        <input type=\"range\" name=\"shadowSize\" title=\"Size\" min=\"0\" max=\"100\" step=\"1\" value=\"10\" condition=\"customStyles &amp;&amp; boxShadow\">\n        <input type=\"range\" name=\"shadowOpacity\" title=\"Opacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.2\" condition=\"customStyles &amp;&amp; boxShadow\">\n        <input type=\"color\" name=\"shadowColor\" title=\"Color\" condition=\"customStyles &amp;&amp; boxShadow\">\n\n        <header condition=\"customStyles\">Border Radius</header>\n        <input type=\"checkbox\" name=\"useBorderRadius\" title=\"Round Border\" condition=\"customStyles\">\n        <input type=\"range\" name=\"borderRadius\" title=\"Size\" min=\"0\" max=\"50\" step=\"1\" value=\"5\" condition=\"customStyles &amp;&amp; useBorderRadius\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" required=\"\">\n            <input type=\"image\" title=\"Background Image\" value=\"@THEME_PATH@/components/_images/background3.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected=\"\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked=\"\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#efefef\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n    <div class=\"container-fluid\">\n        <div class=\"amp-shop\">\n            <h2 mbr-text=\"\" mbr-theme-style=\"display-2\" class=\"amp-shop__title mbr-pb-5 mbr-fonts-style\" mbr-if=\"titleSection\">Our Shop</h2>\n            <div class=\"mbr-row mbr-jc-c\">\n                <!--1-->\n                <div on=\"tap:showLightbox4\" role=\"button\" tabindex=\"0\" class=\"item shop-image mbr-col-lg-4 md-pb mbr-col-md-12 mbr-col-sm-12 \">\n                    <!--Ligtbox-->\n                    <amp-lightbox id=\"showLightbox1\" layout=\"nodisplay\">\n                        <div class=\"lightbox\">\n                            <div class=\"item-box mbr-row\">\n                                <button type=\"button\" class=\"close\" aria-label=\"Close\" on=\"tap:showLightbox3.close\">\n                                    <span aria-hidden=\"true\"></span>\n                                </button>\n                                <div class=\"item-box-img mbr-col-lg-6 mbr-col-md-12 mbr-col-sm-12 mbr-flex mbr-p-4\">\n                                    <amp-img src=\"@PROJECT_PATH@/assets/images/05.jpg\" layout=\"responsive\" width=\"437.5903614457831\" height=\"400\" alt=\"image\" class=\"placeholder-loader\">\n                                        <div placeholder=\"\" class=\"placeholder\"></div>\n                                        <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/05.jpg\" alt=\"\">\n                                    </amp-img>\n                                </div>\n                                <div class=\"item-box-wrapper mbr-col-lg-6 mbr-col-md-12 mbr-col-sm-12 mbr-p-4\">\n\n                                    <div class=\"item-box-header\">\n                                        <h4 class=\"item-box-title item-box-title1 mbr-bold mbr-fonts-style mbr-pb-1\" mbr-theme-style=\"display-4\" data-app-selector=\".item-box-title1\">Leather Belt</h4>\n                                        <div class=\"currency mbr-pb-1\">\n                                            <span mbr-text=\"\" class=\"cur cur1 mbr-fonts-style\" data-app-selector=\".cur1\" mbr-theme-style=\"display-7\">$</span>\n                                            <span mbr-text=\"\" class=\"item-box-price item-box-price1 mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".item-box-price1\">129.99</span>\n                                        </div>\n\n                                    </div>\n                                    <div class=\"item-box-body\">\n                                        <p class=\"item-box-text item-box-text1 mbr-fonts-style mbr-pb-4\" mbr-theme-style=\"display-7\" data-app-selector=\".item-box-text1\">\n                                            Lorem ipsum dolor sit amet, consectetur adipisicing elit. Alias animi,\n                                            debitis, delectus explicabo nesciunt odit officiis, quaerat reiciendis sequi\n                                            sint ullam.\n                                        </p>\n                                    </div>\n                                    <div class=\"item-box-footer\" data-toolbar=\"-mbrBtnMove,-mbrBtnAdd\" mbr-buttons=\"\" mbr-theme-style=\"display-4\"><a href=\"\" class=\"btn item-box-btn btn-black\" target=\"_blank\" data-app-placeholder=\"Type Text\"><span>Buy Now</span></a>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n                    </amp-lightbox>\n                    <!--Item-->\n                    <div class=\"item-wrapper\">\n                        <div class=\"item-content mbr-pb-3\">\n                            <h4 class=\"item-title mbr-fonts-style mbr-bold text-success mbr-pb-1\" mbr-theme-style=\"display-4\">Leather Belt</h4>\n                            <span class=\"cur mbr-fonts-style\" mbr-theme-style=\"display-4\">$</span>\n                            <span class=\"item-price mbr-fonts-style\" mbr-theme-style=\"display-4\">129.99</span>\n                            <p class=\"item-text item-text1 pt-3\" mbr-if=\"showText\" mbr-theme-style=\"display-4\">\n                                Lorem ipsum dolor sit amet, consectetur adipisicing elit. Alias animi,\n                                debitis, delectus explicabo nesciunt odit officiis, quaerat reiciendis sequi\n                                sint ullam.\n                            </p>\n                        </div>\n                        <div class=\"item-img\">\n                            <amp-img src=\"@PROJECT_PATH@/assets/images/05.jpg\" layout=\"responsive\" width=\"437.5903614457831\" height=\"400\" alt=\"image\" class=\"placeholder-loader\">\n                                <div placeholder=\"\" class=\"placeholder\"></div>\n                                <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/05.jpg\" alt=\"\">\n                            </amp-img>\n                        </div>\n                        <div class=\"item-content align-center\">\n\n                            <div class=\"item-box-footer mbr-pt-3\" data-toolbar=\"-mbrBtnMove,-mbrBtnAdd\" mbr-buttons=\"\" mbr-theme-style=\"display-4\"><a href=\"\" class=\"btn item-box-btn btn-black\" target=\"_blank\" data-app-placeholder=\"Type Text\"><span>Buy Now</span></a></div>\n                        </div>\n                    </div>\n                </div>\n                <div class=\"mbr-col-lg-8 mbr-col-md-12 mbr-col-sm-12\">\n                    <div class=\"col2\">\n                        <div class=\"content\">\n                            <h3 class=\"mbr-section-title mbr-fonts-style align-left\" mbr-theme-style=\"display-2\" data-app-selector=\".mbr-section-title\" mbr-if=\"showcardTitle\">Premium Leather</h3>\n                            <div class=\"mbr-section-btn align-left\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\" mbr-buttons=\"\" mbr-theme-style=\"display-4\"><a class=\"btn btn-md btn-white-outline\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\"><span class=\"mobi-mbri mobi-mbri-right mbr-iconfont mbr-iconfont-btn\"></span>Explore</a>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n\n\n                <!---->\n\n\n            </div>\n        </div>\n    </div>\n</section>",
